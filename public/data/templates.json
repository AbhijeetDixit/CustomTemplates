{
  "textTemplate": "<div class='card blue-grey darken-1'><div class='card-content white-text'><span class='card-title'>Card Title</span><p>Hello World</p></div><div class='card-action'><a href='#'>Link 1</a><a href='#'>Link 2</a></div></div>",
  "mixTemplate": "<div class='card medium'><div class='card-image'><img ng-src='{{rootDirectory}}{{content.data}}' /><span class='card-title'></span></div><div class='card-content'><p>I will contain both images and text<p></div><div class='card-action'><a href='#'>Link</a></div></div>",
  "noteTemplate": "<div class='entry-note'><h2>&nbsp;</h2><div class='entry-text'><div class='entry-title'>{{content.title}}</div><div class='entry-copy'>{{content.data}}</div></div></div>",
  "textOnlyTemplate" : "<div class='card blue-grey darken-1'><div class='card-content white-text'><div class='row'><span class='card-title'><h5 class='col s11'>{{content.title}}</h5></span><span class='badge deep-purple lighten-5'>{{content.type}}</span></div><div class='row'><div class='s12'><div class='chip'><img src='images/abc5.jpg' alt='some'>Tag 1</div><div class='chip'><img src='images/abc6.jpg' alt='some'>Tag 2</div></div><span class='white-text col s6'>by : Poster @ {{content.timestamp}}</span></div><div class='divider'></div><p style='margin-top:10px; margin-left:5px;'>{{content.statement}}</p></div><div class='card-action'><a href='#'>Like</a><a href='#'>Comment</a><a href='#'>Share</a></div></div>",
  "singleImageTemplate" : "<div class='card'><div class='card-image waves-effect waves-block waves-light'><img class='activator' src='{{rootDirectory}}{{content.cardimage}}' style='height:250px;'></div><div class='card-content'><span class='card-title activator grey-text text-darken-4'>Card Title<i class='material-icons right'>more_vert</i></span><div class='card-action'><p><a href='#'>Like</a><a href='#'>Comment</a><a href='#'>Share</a></p></div></div><div class='card-reveal'><span class='card-title grey-text text-darken-4'><h5 class='col s10'>Card Title</h5><span class='badge deep-purple lighten-5 col s1'>{{content.type}}</span><i class='material-icons right'>close</i></span><p>{{content.statement}}</p></div></div></div>"
}