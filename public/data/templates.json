{
  "textTemplate": "<div class='card blue-grey darken-1'><div class='card-content white-text'><span class='card-title'>Card Title</span><p>Hello World</p></div><div class='card-action'><a href='#'>Link 1</a><a href='#'>Link 2</a></div></div>",
  "mixTemplate": "<div class='card medium'><div class='card-image'><img ng-src='{{rootDirectory}}{{content.data}}' /><span class='card-title'></span></div><div class='card-content'><p>I will contain both images and text<p></div><div class='card-action'><a href='#'>Link</a></div></div>",
  "noteTemplate": "<div class='entry-note'><h2>&nbsp;</h2><div class='entry-text'><div class='entry-title'>{{content.title}}</div><div class='entry-copy'>{{content.data}}</div></div></div>"
}